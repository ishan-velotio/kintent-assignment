  "scripts": {
    "clean": "rm -rf build",
    "copyStatic": "cp -r api/swagger build/api && cp -r config build",
    "compile": "./node_modules/typescript/bin/tsc && npm run copyStatic",
    "migrate": "mkdir -p build/migrations && ./node_modules/sequelize-cli/lib/sequelize db:migrate --migrations-path=build/migrations",
    "migrate-and-start": "node build/app.js",
    "compile-and-start": "npm run compile && npm run migrate-and-start",
    "swagger-verify": "swagger project verify",
    "lint": "node_modules/tslint/bin/tslint -r api/**/*.ts  api/**/**/*.ts api/**/**/**/*.ts api/**/**/**/**/*.ts",
    "lint-fix": "tslint -r api/**/*.ts  api/**/**/*.ts api/**/**/**/*.ts api/**/**/**/**/*.ts --fix",
    "swagger-test": "swagger project test",
    "test-coverage": "NODE_ENV=test TS_NODE_FILES=true nyc --reporter=text --reporter=html node_modules/mocha/bin/mocha -r ts-node/register test/unit/spec_helper.ts 'test/unit/**/*.spec.ts'",
    "create-migration-file": "./node_modules/sequelize-cli/lib/sequelize migration:generate --name $filename && for f in ./migrations/*.js; do  cat sequelize/migration-template.ts > \"$f\"; mv -- \"$f\" \"${f%.js}.ts\"; done",
    "dev": "npm run clean && ./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile-and-start\"",
    "debug": "npm run clean && npm run compile && DEBUG=* node inspect build/app.js",
    "unit-tests": "NODE_ENV=test TS_NODE_FILES=true node_modules/mocha/bin/mocha -r ts-node/register test/unit/spec_helper.ts 'test/unit/**/*.spec.ts'"
  }
